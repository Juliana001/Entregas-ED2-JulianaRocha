/*
O código abaixo realiza a impressão do vetor na ordem inversa.
1 - comente o código
2 - faça uma modificação para que seja possível inserir um vetor de n elementos.
*/

#include <stdio.h>

int main() {
    int n;
    scanf("%d", &n);
    int vetor[n];
    int i, temp;
    for(i=0; i<n;i++){
        scanf("%d", &vetor[i]);
    }
    
     for (i = 0; i < n/2; i++) {
        temp = vetor[i];
        vetor[i] = vetor[(n - 1) - i];
        vetor[(n-1) - i] = temp;
    }

    printf("Vetor invertido:\n");
    for (i = 0; i < n; i++) {
        printf("%d ", vetor[i]);
    }

    return 0;
}
